coroutine.wrap(DPCA_fake_script)()
local function UVMN_fake_script() -- Tab2.LocalScript 
	local script = Instance.new('LocalScript', Tab2)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local par = script.Parent
	local player = game:GetService("Players").LocalPlayer
	local char = player.Character or player.Character:Wait()
	local backpack = player:WaitForChild("Backpack")
	-- Function to get rotation amount dynamically from TextBox
	local function getRotationAmount()
		local text = par.Scale.Text
		local number = tonumber(text)
		if number then
			return number
		else
			return 45  -- default if invalid
		end
	end
	
	local directionNames = {"Left", "Right", "Up", "Down", "Forward", "Back"}
	
	for _, name in ipairs(directionNames) do
		local button = par:WaitForChild(name)
		if button:IsA("TextButton") then
			button.MouseButton1Click:Connect(function()
				-- Get tool when button is clicked
				local tool = char:FindFirstChildOfClass("Tool")
				if not tool then
					return
				end
	
				local ROTATION_AMOUNT = getRotationAmount() -- get current value
				local directions = {
					Left = CFrame.Angles(0, math.rad(-ROTATION_AMOUNT), 0),
					Right = CFrame.Angles(0, math.rad(ROTATION_AMOUNT), 0),
					Up = CFrame.Angles(math.rad(ROTATION_AMOUNT), 0, 0),
					Down = CFrame.Angles(math.rad(-ROTATION_AMOUNT), 0, 0),
					Forward = CFrame.Angles(0, 0, math.rad(-ROTATION_AMOUNT)),
					Back = CFrame.Angles(0, 0, math.rad(ROTATION_AMOUNT)),
				}
	
				tool.Grip = tool.Grip * directions[name]
				if tool then
					tool.Parent = backpack
					tool.Parent = char
				end
			end)
		end
	end
	
end
coroutine.wrap(UVMN_fake_script)()
local function ODVU_fake_script() -- Tab.LocalScript 
	local script = Instance.new('LocalScript', Tab)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	script.Parent.MouseButton1Click:Connect(function()
		local tab1 = script.Parent.Parent.Tab1
		local tab2 = script.Parent.Parent.Tab2
		tab1.Visible = not tab1.Visible
		tab2.Visible = not tab1.Visible  -- hide Tab2 if Tab1 is visible
	end)
end
coroutine.wrap(ODVU_fake_script)()
local function RSMZ_fake_script() -- Spin.LocalScript 
	local script = Instance.new('LocalScript', Spin)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	script.Parent.MouseButton1Click:Connect(function()
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
	
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoid = character:WaitForChild("Humanoid")
		local backpack = player:WaitForChild("Backpack")
	
		-- find a tool (first one found)
		local tool = character:FindFirstChildOfClass("Tool")
			or backpack:WaitForChild(backpack:GetChildren()[1] and backpack:GetChildren()[1].Name)
	
		if tool then
			tool.Parent = backpack
			tool.Parent = character
		end
	
		-- rotate the grip
		while task.wait() do
			if tool and tool.Parent == character then
				tool.Grip *= CFrame.Angles(0, math.rad(-1), 0)
			end
		end
	end)
end
coroutine.wrap(RSMZ_fake_script)()
